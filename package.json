{
  "name": "share-expenses-app",
  "version": "1.0.0",
  "description": "Web app to share living expenses",
  "main": "dist/server",
  "scripts": {
    "next:dev": "next",
    "next:build": "next build",
    "next:start": "next start",
    "clean": "rm -rf dist",
    "build": "npm -s run clean && npm -s run generate && tsc --project tsconfig-api.json ",
    "start": "node -r dotenv/config dist/server",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus",
    "generate:prisma": "prisma generate --schema=./prisma/schema.prisma",
    "generate:nexus": "ts-node --transpile-only -r dotenv/config handlers/schema",
    "postinstall": "npm -s run generate",
    "dev": "ts-node-dev --no-notify --respawn --transpileOnly -r dotenv/config --project tsconfig-api.json handlers/server"
  },
  "keywords": [],
  "author": "Miki Masumoto",
  "license": "ISC",
  "devDependencies": {
    "@prisma/cli": "^2.0.0-beta.4",
    "@types/node": "^13.13.8",
    "@types/react": "^16.9.35",
    "@types/react-datepicker": "^2.11.0",
    "ts-node": "^8.10.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.5",
    "@nexus/schema": "^0.13.1",
    "@prisma/client": "^2.0.0-beta.5",
    "@rebass/forms": "^4.0.6",
    "@rebass/preset": "^4.0.5",
    "@theme-ui/presets": "^0.3.0",
    "@types/rebass": "^4.0.6",
    "@types/theme-ui": "^0.3.2",
    "apollo-boost": "^0.4.9",
    "apollo-server": "^2.13.1",
    "apollo-server-lambda": "^2.13.1",
    "bootstrap": "^4.5.0",
    "create-react-class": "^15.6.3",
    "date-fns": "^2.14.0",
    "dotenv": "^8.2.0",
    "emotion-theming": "^10.0.27",
    "gql-query-builder": "^3.3.0",
    "graphql": "^15.0.0",
    "moment": "^2.26.0",
    "next": "^9.4.1",
    "next-redux-wrapper": "^6.0.0",
    "nexus-prisma": "^0.12.0",
    "node-fetch": "^3.1.1",
    "react": "^16.13.1",
    "react-bootstrap": "^1.0.1",
    "react-datepicker": "^2.16.0",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-vis": "^1.11.7",
    "rebass": "^4.0.7",
    "redux": "^4.0.5",
    "serverless-dotenv-plugin": "^2.4.2",
    "styled-components": "^5.1.0",
    "theme-ui": "^0.3.1"
  }
}
